<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="360.0" prefWidth="560.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1">
   <top>
      <ToolBar prefHeight="40.0" prefWidth="200.0" BorderPane.alignment="CENTER">
        <items>
          <Button mnemonicParsing="false" onAction="#get" text="Recibir correo" />
            <Button mnemonicParsing="false" onAction="#send" text="Enviar correo" />
        </items>
      </ToolBar>
   </top>
   <center>
      <ListView fx:id="listView" prefHeight="200.0" prefWidth="449.0" BorderPane.alignment="CENTER" />
   </center>
</BorderPane>
